//嵌套if结构
import java.util.*;
public class T{
	public static void main(String[] args){
		System.out.println("请输入你的性别？（male/female）");
		Scanner in=new Scanner(System.in);
		String sex=in.next();
		if(sex.equals("female")){
//面向对象里的知识，在字符串作比较时，使用
			System.out.println("Your age:");
			int age=in.nextInt();
			if(age>=18){
				System.out.println("You are pretty girl!");
			}else{
				System.out.println("You are little girl!");
			}
		}else{
			System.out.println("You are pretty boy!");
		}
	}
}
