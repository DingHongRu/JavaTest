import java.util.Scanner;
public class T{
	public static void main(String[] args){
		Scanner in=new Scanner(System.in);
		for(;;){//死循环
			System.out.println("Go on?");
			String flag=in.next();
			if(flag.equals("yes")){
				System.out.println("Well,dame it!Come on!");
			}else{
				break;//循环结构和条件结构可以互相嵌套
			}
		}
	}
}
