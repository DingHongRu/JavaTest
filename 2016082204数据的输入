import java.util.Scanner;
public class T{
	public static void main(String[] args){
		Scanner in=new Scanner(System.in);
		System.out.println("请输入你的姓名：");
		String name=in.next();
		System.out.println("请输入你的年龄：");
		int age=in.nextInt();
		System.out.println("Name:"+name);
		System.out.println("Age:"+age);
	}
}
