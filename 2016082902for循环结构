import java.util.*;
public class Test{
	public static void main(String[] args){
		Scanner in=new Scanner(System.in);
		double sum=0;
		for(int j=0;j<5;j++){
			System.out.println("请输入你的第"+(j+1)+"个成绩：");
			double score=in.nextDouble();
			sum=sum+score;
		}
		System.out.println("平均分为："+sum/5);
	}
}
